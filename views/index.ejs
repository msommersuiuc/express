<!DOCTYPE html>
<html>

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width,initial-scale=1">
<link rel="stylesheet" href="/stylesheets/bootstrap.min.css" />
</head>

<body>
  <div class="container">
    <div class="card mt-4">
      <div class="card-header">
        <h2>Create a Card</h2>
      </div>
      <div class="card-body">
        <div class="mb-3">
          <label>From:</label>
          <input id="from-input" type="text" class="form-control">
        </div>
        <div class="mb-3">
          <label>To:</label>
          <input id="to-input" type="text" class="form-control">
        </div>
        <div class="mb-3">
          <label>To:</label>
          <input type="text" class="form-control">
        </div>
      </div>
      <div class="card-footer">
        <button id="submit-btn" type="button" class="btn btn-success">Generate</button>
      </div>
    </div>
  </div>
</body>
<script src="/javascripts/bootstrap.min.js"></script>
<script src="/javascripts/jquery.min.js"></script>

<script type="text/javascript">
  $(document).ready(() => {
    $("#submit-btn").on("click", () => {
      $.post("/submit", { from: $("#from-input").val(), to: $("#to-input").val() }, data => {

      });
    });
  });
</script>
</html>